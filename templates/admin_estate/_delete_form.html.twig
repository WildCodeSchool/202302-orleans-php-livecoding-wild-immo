<form method="post" action="{{ path('app_admin_estate_delete', {'id': estate.id}) }}" onsubmit="return confirm('Êtes vous sûr de vouloir supprimer ce bien ?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ estate.id) }}">
	<button class="btn btn-sm btn-outline-danger">
        {% include 'icons/trash.svg' %}
		Supprimer
	</button>
</form>
